<?xml version="1.0" encoding="ISO-8859-1"?>
<web-app xmlns="http://java.sun.com/xml/ns/j2ee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee
 http://java.sun.com/xml/ns/j2ee/web-app_2_4.xsd"
 version="2.4">

    <servlet>
        <servlet-name>authenticate</servlet-name>
        <servlet-class>authenticate</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>authenticate</servlet-name>
        <url-pattern>/authenticate</url-pattern>
    </servlet-mapping>

     <servlet>
        <servlet-name>recordpage</servlet-name>
        <servlet-class>recordpage</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>recordpage</servlet-name>
        <url-pattern>/recordpage</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>upload</servlet-name>
        <servlet-class>upload</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>upload</servlet-name>
        <url-pattern>/upload</url-pattern>
    </servlet-mapping>

     <servlet>
        <servlet-name>adduser</servlet-name>
        <servlet-class>adduser</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adduser</servlet-name>
        <url-pattern>/adduser</url-pattern>
    </servlet-mapping>

  <servlet>
        <servlet-name>addperson</servlet-name>
        <servlet-class>addperson</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addperson</servlet-name>
        <url-pattern>/addperson</url-pattern>
    </servlet-mapping>

  <servlet>
        <servlet-name>adddoctor</servlet-name>
        <servlet-class>adddoctor</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>adddoctor</servlet-name>
        <url-pattern>/adddoctor</url-pattern>
    </servlet-mapping>

</web-app>
